//- Includes
include ../elements/_ui-thumbnail-custom
include ../elements/_ui-isotope

// Portfolio Grid 3 columns
section.section.section-lg.bg-default
	.container
		+isotope(['All', 'Health Care', 'Diagnostics', 'Team'],{
			"group": "gallery",
			"layout": "fitRows",
			"alignClass": "isotope-filters-horizontal"
		}, "isotope-1")
			.row.row-30(data-lightgallery="group")
				.col-12.col-md-6.col-lg-4.isotope-item(data-filter="Health Care")
					+thumbnail-custom({
						"type": "thumbnail",
						"title": "Quality Health Care",
