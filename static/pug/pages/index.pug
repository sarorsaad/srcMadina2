	//Sheldue
	section.bg-default-liac.bg-white-liac.section.section-md.pt-xl-0
		.container.section-top-34.section-lg-top-0
			.row.no-gutters.justify-content-sm-center.justify-content-xl-start.offset-lg-top-34-negative.sheldue.text-sm-left.to-front.row-30
				.col-md-8.col-lg-5.col-xl-3
					.sheldue-item.first
						.sheldue-item-body
							.icon.icon-xs.mdi.mdi-calendar-clock.text-white-50
							h6.d-inline-block.inset-left-10.text-white opening hours
							hr
							.row.offset-top-24.justify-content-sm-between.text-gray-light
								.col-sm-4 Mon–Fri
								.col-sm-7.offset-top-10.offset-xs-top-0.text-sm-right 8:00am–7:00pm
								.col-sm-4.offset-top-30.offset-xs-top-10 Saturday
								.col-sm-7.offset-top-10.text-sm-right 9:00am–5:00pm
								.col-sm-4.offset-top-30.offset-xs-top-10 Sunday
								.col-sm-7.offset-top-10.text-sm-right 9:00am–3:00pm
						.sheldue-more
							a(href="timetable.html").btn.btn-white.btn-block.btn-rect view timetable

				.col-md-8.col-lg-5.col-xl-3
					.sheldue-item.light
						.sheldue-item-body
							.icon.icon-xs.mdi.mdi-calendar-today.text-white-50
							h6.d-inline-block.inset-left-10.text-white doctors’ timetable
							hr
							.offset-top-24.text-gray-light
								p The following is for guidance only to help you plan your appointment with a preferred doctor or nurse. It does not guarantee availability as the doctors or nurses may sometimes be attending to other duties.
						.sheldue-more
							a(href="timetable.html").btn.btn-white.btn-block.btn-rect view timetable

				.col-md-8.col-lg-5.col-xl-3
					.sheldue-item
						.sheldue-item-body
							.icon.icon-xs.mdi.mdi-phone.text-white-50
							h6.d-inline-block.inset-left-10.text-white appointments
							hr
							.offset-top-24.text-gray-light
								p The first step towards a healthy life  is to schedule an appointment. Please contact our office by phone or complete the appointment request form.
						.sheldue-more
							a(href="make-an-appointment.html").btn.btn-white.btn-block.btn-rect make an appointment

				.col-md-8.col-lg-5.col-xl-3
					.sheldue-item.light.last
						.sheldue-item-body
							.icon.icon-xs.icon-emergency-01.text-white-50
							h6.d-inline-block.inset-left-10.text-white emergency cases
							hr
							.offset-top-24
								h5.font-weight-bold
									a(href="tel:#").text-white 1-800-1234-567
								p.text-gray-light Call us!
						.sheldue-more
							a(href="contacts.html").btn.btn-white.btn-block.btn-rect Contact page


	include ../sections/_section-what-makes-us-different

	// Material Parallax
	section.parallax-container( data-parallax-img='images/background-03-1920x939.jpg')
		.parallax-content.section-98.section-sm-110.context-dark
			.container.text-left
				.row.justify-content-sm-center.justify-content-lg-start
					.col-md-10.col-lg-8.col-xl-5
