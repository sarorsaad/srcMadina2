extends  ../_skeleton

//- Includes
include ../elements/_ui-breadcrumb-modern
include ../elements/_ui-pagination-classic
include ../elements/_ui-thumbnail-custom
include ../elements/_ui-isotope

//- Title
block variables
	- pageName = 'Cobbles Gallery'

//- Page
block page

	include ../sections/_header-subpage

	+breadcrumbs

	section.section-lg.section.bg-default.section-cobbles-gallery
		.container-fluid.inset-lg-left-0.inset-lg-right-0
			.row.justify-content-sm-center.justify-content-lg-start
				.col-sm-8.col-lg-12
					+isotope(['All', 'Health Care', 'Diagnostics', 'Team'],{
						"group": "gallery",
						"layout": "masonry",
						"alignClass": "isotope-filters-horizontal"
					}, "isotope-1")
						.row.row-condensed(data-lightgallery="group")
							.col-12.col-md-6.isotope-item(data-filter="Health Care")
								+thumbnail-custom({
									"type": "thumbnail",
									"title": "Quality Health Care",
									"thumb": {"src": "images/gallery/gallery-01-370x240.jpg", "width": 370, "height": 240, "alt": ""},
									"target": {
										"src": "images/gallery/originals/gallery-01_original.jpg",
										"width": 1200,
										"height": 800,
										"alt": ""
									}
								})
